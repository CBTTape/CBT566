&GEN     SETC  'GEN'
***********************************************************************
*                                                                     *
*                                                                     *
***********************************************************************
         SPACE 3
*--------------------------------------------------------------------*
*              REGISTER AND OTHER EQUATES                            *
*--------------------------------------------------------------------*
         SPACE 3
R0       EQU   0
R1       EQU   1
R2       EQU   2
R3       EQU   3
R4       EQU   4
R5       EQU   5
R6       EQU   6
R7       EQU   7
R8       EQU   8
R9       EQU   9
R10      EQU   10
R11      EQU   11
R12      EQU   12
R13      EQU   13
R14      EQU   14
R15      EQU   15
         SPACE 2
*--------------------------------------------------------------------*
*              $$SUB MACRO         NEEDED BY PRTDUMP                 *
*--------------------------------------------------------------------*
         PRINT OFF
         MACRO
&L       $$SUB
&L       STM   R14,R12,12(R13)     SAVE REGS
         LA    R14,72(,R13)        NEXT SAVE
         ST    R14,8(,R13)         CHAIN
         ST    R13,4(,R14)          SAVE
         LR    R13,R14             NEW SAVE
         MEND
         PRINT ON
         EJECT
DUMPTCT  TITLE 'DUMPTCT - '
DUMPTCT  AMODE 31
DUMPTCT  RMODE 24
DUMPTCT  CSECT
         SAVE  (14,12),T,'DUMPTCT_&SYSDATC._&SYSTIME.'
         LR    R12,R15            POINT BASE REGISTER TO ENTRY POINT
         USING DUMPTCT,R12
         LA    R2,SAVEAREA        POINT TO SAVEAREA
         ST    R13,SAVEAREA+4     POINTER TO CALLERS SAVEAREA
         ST    R2,8(R13)          POINTER TO CALLED SAVEAREA
         LR    R13,R2             STANDARD POINTER TO SAVEAREA
*--------------------------------------------------------------------*
*              GET TCT ADDRESS                                       *
*--------------------------------------------------------------------*
         L     R2,PSATOLD-PSA(0)   GET TCB ADDRESS
         L     R2,TCBTCT-TCB(R2)   GET TCT ADDRESS
         LR    R4,R2               GET TCB
         LA    R0,TCTBIG           PICK UP ENTRY LENGTH
         CVD   R0,DEC
         MVC   DEC+08(6),=X'402020202120'
         ED    DEC+08(6),DEC+5
         MVC   MSG1DEC(4),DEC+08+2
         ST    R0,DEC
         UNPK  DEC+8(05),DEC+2(3)
         MVC   MSG1HEX(3),DEC+8+1
         TR    MSG1HEX(3),TRANS-240
         LA    R1,MSG1
         BAL   R14,PRTDMSG
         LR    R1,R4
         BAL   R14,PRTDUMP
CKMVS    DS    0H
         LA    R0,TCTBIG           PICK UP ENTRY LENGTH
         LA    R1,MSG2
         BAL   R14,PRTDMSG
         L     R13,4(,R13)
         RETURN (14,12),RC=0
*--------------------------------------------------------------------*
*              WTO MESSAGES                                          *
*--------------------------------------------------------------------*
MSG1     WTO   '*** DUMP OF TCT - LENGTH=XXXX/XXX ***',MF=L,           X
               ROUTCDE=11
MSG1DEC  EQU   MSG1+29,4
MSG1HEX  EQU   MSG1+34,3
MSG2     WTO   '*** END  OF TCT ***',MF=L,ROUTCDE=11
*--------------------------------------------------------------------*
*              DATA AREAS                                            *
*--------------------------------------------------------------------*
SAVEAREA DS    (4*9)D'0'
DEC      DS    2D
TRANS    DC    C'0123456789ABCDEF'
         SPACE 2
         COPY  PRTDUMP
         PRINT GEN
         MVSAREAS TCT=YES,                                             X
               TCB=YES,                                                X
               PSA=YES,                                                X
               LISTGEN=YES
         END
